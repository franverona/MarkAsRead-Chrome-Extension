{
  "name": "Mark as read",
  "version": "2.2",
  "manifest_version": 2,
  
  "description": "To mark as read individual items on Google Reader",
  
  "background": { "scripts": ["mark.js"] },
  "content_scripts": [{
	"matches": ["http://www.google.com/reader*","http://www.google.es/reader*"],
	"js": ["mark.js"] 
  }]
}